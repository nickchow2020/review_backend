{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "username": "hello,world",
            "password": "123 main st",
            "first_name": "hello,world",
            "last_name": "1234311234",
            "email": 1,
            "is_admin": true,
            "avatar_url": "91792"
        }
    ],
    "required": [
        "username",
        "password",
        "first_name",
        "last_name",
        "email",
        "is_admin",
        "avatar_url"
    ],
    "properties": {
        "username": {
            "$id": "#/properties/username",
            "type": "string",
            "title": "The username schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "hello,world"
            ]
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The password schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "123 main st"
            ]
        },
        "first_name": {
            "$id": "#/properties/first_name",
            "type": "string",
            "title": "The first_name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "hello,world"
            ]
        },
        "last_name": {
            "$id": "#/properties/last_name",
            "type": "string",
            "title": "The last_name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "1234311234"
            ]
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "shumin_z@gmail.com"
            ]
        },
        "is_admin": {
            "$id": "#/properties/is_admin",
            "type": "boolean",
            "title": "The is_admin schema",
            "description": "An explanation about the purpose of this instance.",
            "default": false,
            "examples": [
                true
            ]
        },
        "avatar_url": {
            "$id": "#/properties/avatar_url",
            "type": "string",
            "title": "The avatar_url schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "91792"
            ]
        }
    },
    "additionalProperties": true
}